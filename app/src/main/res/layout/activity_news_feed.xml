<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Contents.NewsFeedActivity">

    <android.support.constraint.Guideline
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline11" app:layout_constraintGuide_end="80dp"/>

    <android.support.constraint.Guideline
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintGuide_begin="60dp" android:id="@+id/guideline14"/>

    <android.support.constraint.Guideline
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline12" app:layout_constraintGuide_end="8dp"/>
    <android.support.constraint.Guideline
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintGuide_begin="8dp" android:id="@+id/guideline13"/>

    <Button
            android:text="W"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/writeButton" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline11" app:layout_constraintEnd_toStartOf="@+id/guideline12"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintStart_toStartOf="@+id/guideline13" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.971"
            app:layout_constraintVertical_bias="0.375"/>

    <Button
            android:text="My"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/myButton" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline11" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="@+id/guideline13"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toStartOf="@+id/guideline12" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.783"
            app:layout_constraintVertical_bias="0.375"/>
    <Button
            android:text="N"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/notiButton" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline11" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="@+id/guideline13"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toStartOf="@+id/guideline12" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.595"
            app:layout_constraintVertical_bias="0.375"/>
    <Button
            android:text="C"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/chatButton" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline11" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="@+id/guideline13"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toStartOf="@+id/guideline12" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.407"
            app:layout_constraintVertical_bias="0.375"/>
    <Button
            android:text="S"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/searchButton" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline11" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="@+id/guideline13"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toStartOf="@+id/guideline12" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.219"
            app:layout_constraintVertical_bias="0.375"/>
    <Button
            android:text="H"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:id="@+id/HomeButton" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline11" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="@+id/guideline13"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toStartOf="@+id/guideline12" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.031"
            app:layout_constraintVertical_bias="0.375"/>
    <ScrollView
            android:layout_width="0dp"
            android:layout_height="0dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline14" app:layout_constraintStart_toStartOf="@+id/guideline13"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toStartOf="@+id/guideline12" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline11">
        <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                      android:orientation="vertical">
            <Button
                    android:text="Button"
                    android:layout_width="match_parent"
                    android:layout_height="100dp" android:id="@+id/button1"/>
            <Button
                    android:text="Button"
                    android:layout_width="match_parent"
                    android:layout_height="100dp" android:id="@+id/button2"/>
            <Button
                    android:text="Button"
                    android:layout_width="match_parent"
                    android:layout_height="100dp" android:id="@+id/button3"/>
            <Button
                    android:text="Button"
                    android:layout_width="match_parent"
                    android:layout_height="100dp" android:id="@+id/button4"/>
            <Button
                    android:text="Button"
                    android:layout_width="match_parent"
                    android:layout_height="100dp" android:id="@+id/button5"/>
            <Button
                    android:text="Button"
                    android:layout_width="match_parent"
                    android:layout_height="100dp" android:id="@+id/button6"/>
            <Button
                    android:text="Button"
                    android:layout_width="match_parent"
                    android:layout_height="100dp" android:id="@+id/button7"/>

        </LinearLayout>
    </ScrollView>


</android.support.constraint.ConstraintLayout>